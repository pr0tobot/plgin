# PLGN Proxy Environment Variables
# Copy to .env and fill in actual values, then create Modal secret:
# modal secret create plgn-secrets --from-dotenv=proxy/.env

# GitHub token with repo access to PR0TO-IDE/plgn-registry
GITHUB_TOKEN=your_github_token_here

# Nia API key for semantic search
NIA_API_KEY=your_nia_api_key_here
NIA_API_URL=https://apigcp.trynia.ai/

# Admin token for registry updates (generate a secure random string)
ADMIN_TOKEN=your_admin_token_here
