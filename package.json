    "clean": "rimraf cli/dist",
    "test": "vitest"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "diff": "^5.2.0",
    "dotenv": "^16.6.1",
    "fs-extra": "^11.2.0",
    "openai": "^6.1.0",
    "undici": "^6.19.8",
    "ora": "^7.0.1",
    "tar": "^6.2.0",
    "yaml": "^2.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/diff": "^5.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.30",
    "@types/tar": "^6.1.11",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vitest": "^1.5.3"
  }
}
