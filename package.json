{
  "name": "plgin",
  "version": "2.0.8",
  "description": "Semantic feature extraction and code integration using vector embeddings - language-agnostic CLI tool",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pr0tobot/plgin.git"
  },
  "homepage": "https://github.com/pr0tobot/plgin",
  "bugs": {
    "url": "https://github.com/pr0tobot/plgin/issues"
  },
  "keywords": [
    "semantic-search",
    "embeddings",
    "vector-search",
    "code-generation",
    "feature-extraction",
    "language-agnostic",
    "cli",
    "code-reuse",
    "developer-tools",
    "code-integration"
  ],
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "README.md",
    "SETUP.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "type": "module",
  "bin": {
